<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
                               name="Asciidoc Report">
    <description>Renders Asciidoc files containing rules to HTML reports including results as tables, diagrams or links to external files.</description>
    <report>
        <class id="asciidoc">com.buschmais.jqassistant.plugin.asciidocreport.AsciidocReportPlugin</class>
        <class id="plantuml-component-diagram">com.buschmais.jqassistant.plugin.asciidocreport.plantuml.component.ComponentDiagramReportPlugin</class>
        <class id="plantuml-sequence-diagram">com.buschmais.jqassistant.plugin.asciidocreport.plantuml.sequence.SequenceDiagramReportPlugin</class>
        <class id="plantuml-class-diagram">com.buschmais.jqassistant.plugin.asciidocreport.plantuml.clazz.ClassDiagramReportPlugin</class>
    </report>
</jqa-plugin:jqassistant-plugin>
